{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [
          ".ios.ts",
          ".android.ts",
          ".ts",
          ".ios.tsx",
          ".android.tsx",
          ".tsx",
          ".jsx",
          ".js",
          ".json"
        ],
        "alias": {
          "@actions": "./src/actions",
          "@assets": "./src/assets",
          "@components": "./src/components",
          "@containers": "./src/containers",
          "@constants": "./src/constants",
          "@baseStyle": "./src/styles/baseStyle",
          "@commonStyle": "./src/styles/commonStyle",
          "@utils": "./src/utils"
        }
      }
    ]
  ]
}
